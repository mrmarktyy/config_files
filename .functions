# Rename tab
function tabname {
  printf "\e]1;$1\a"
}

# Rename winname
function winname {
  printf "\e]2;$1\a"
}

function info {
  printf "\r  [\033[00;34mINFO\033[0m ] $1\n"
}

function success {
  printf "\r\033[2K  [\033[00;32mOK\033[0m ] $1\n"
}

function fail {
  printf "\r\033[2K  [\033[0;31mFAIL\033[0m] $1\n"
  echo ''
  exit 1
}
